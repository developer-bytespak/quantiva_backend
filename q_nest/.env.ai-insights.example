# ==========================================
# AI INSIGHTS CONFIGURATION
# ==========================================

# OpenAI API (Primary Provider)
OPENAI_API_KEY=your_openai_api_key_here

# Gemini API (Fallback Provider)
GEMINI_API_KEY=your_gemini_api_key_here

# AI Generation Limits
AI_NEWS_LIMIT=3
TRENDING_ASSETS_LIMIT=4
MAX_CONCURRENT_AI_REQUESTS=3
AI_CACHE_TTL_MS=3600000

# Provider Configuration
USE_OPENAI_PRIMARY=true  # Set to false to use Gemini as primary
ENABLE_AI_FALLBACK=true  # Set to false to disable fallback

# ==========================================
# DATA CLEANUP CONFIGURATION
# ==========================================

# Data Retention (days)
DATA_RETENTION_DAYS=5

# Cleanup Batch Size
CLEANUP_BATCH_SIZE=100
